{
  "name": "jjeanio",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./dist-firebase/firebase.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "predeploy": "npm run build:prod",
    "deploy": "firebase deploy",
    "postbuild:prod": "node postprocess.js && ngsw-config dist src/ngsw-config.json",
    "build:prod": "concurrently 'npm run build:client:prod' 'npm run build:server:ng' 'npm run build:server:firebase' --names 'CLIENT,SERVER:NG,SERVER:FB' -c 'green.bold,blue.bold,yellow.bold'",
    "build:client:prod": "ng build --prod",
    "build:client": "ng build --aot",
    "build:server:ng": "ng build --app server --aot",
    "build:server:ts": "tsc --project src/tsconfig.ts.json",
    "build:server:firebase": "tsc --project src/tsconfig.firebase.json",
    "watch:client": "npm run build:client -- -w",
    "watch:server:ng": "npm run build:server:ng -- -w",
    "watch:server:ts": "npm run build:server:ts -- -w",
    "watch:server:firebase": "npm run build:server:firebase -- -w",
    "watch": "concurrently 'npm run watch:client' 'npm run watch:server:ng' 'npm run watch:server:ts' --names 'CLIENT,SERVER:NG,SERVER:TS' -c 'green.bold,blue.bold,yellow.bold'",
    "server": "nodemon ./dist-ts/server.js",
    "coverage": "http-server coverage",
    "test": "ng test --code-coverage",
    "lint": "ng lint --type-check",
    "commitlint": "commitlint --from master",
    "e2e": "ng e2e --aot"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.4",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angular/service-worker": "^5.0.0",
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "firebase": "^4.8.2",
    "firebase-admin": "^5.8.0",
    "firebase-functions": "^0.8.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.4",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@commitlint/travis-cli": "^6.0.2",
    "@types/express": "^4.11.0",
    "@types/jasmine": "^2.8.4",
    "@types/jasminewd2": "^2.0.2",
    "@types/node": "^9.3.0",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "http-server": "^0.11.1",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.14.11",
    "protractor": "^5.2.2",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
